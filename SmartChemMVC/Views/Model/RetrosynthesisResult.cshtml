

@model RetrosynthesisResult

<h2>Retrosynthesis Result</h2>

@if (Model != null && Model.Paths.Any())
{
    @for (int i = 0; i < Model.Paths.Count; i++)
    {
        <h3>Path @i</h3>
        <img src="@($"https://chemgeek.live/image/{Model.Paths[i]}")" alt="Path Image" />
        <p>Confidence: @Model.Confidences[i]</p>
    }
}
